{
  "name": "autopilot-data-platform",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:staging": "vue-tsc --noEmit && vite build --mode staging",
    "preview": "vite preview",
    "prepare": "husky install",
    "prettier": "prettier --write ./**/*.{js,json,vue}",
    "lint:eslint": "eslint \"src/**/*.{vue,js,jsx}\" --fix",
    "lint:stylelint": "stylelint src/**/*.less --fix --custom-syntax postcss-less"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/three": "^0.144.0",
    "@typescript-eslint/parser": "^5.37.0",
    "autoprefixer": "^10.4.11",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-define-config": "^1.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.4.0",
    "husky": "^8.0.1",
    "less": "^4.1.3",
    "lint-staged": "^13.0.3",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "prettier": "^2.7.1",
    "stylelint": "^14.11.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vue-eslint-parser": "^9.1.0",
    "vue-template-compiler": "^2.7.10"
  },
  "dependencies": {
    "@vitejs/plugin-vue2": "^2.0.0",
    "axios": "^0.27.2",
    "lodash": "^4.17.21",
    "meg-ui": "1.0.0-beta.86",
    "pinia": "^2.0.22",
    "three": "^0.144.0",
    "vue": "^2.7.10",
    "vue-router": "^3.5.2",
    "vue-tsc": "^0.40.13"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.vue": [
      "stylelint --fix",
      "prettier --write",
      "eslint --fix"
    ],
    "*.{less,css}": [
      "stylelint --fix",
      "prettier --write"
    ]
  }
}
